{
  "codeLens.enable": true,
  "suggest.autoTrigger": "always",
  "suggest.enablePreview": true,
  "diagnostic.displayByAle": true,
  "suggest.floatEnable": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "signature.enable": true,
  "signature.target": "float",
  //"diagnostic.messageTarget": "float",
  "coc.preferences.hoverTarget": "float",
  "coc.source.buffer.ignoreGitignore": false,
  //"suggest.snippetIndicator": "â–º",
  "tsserver.enable": true,
  "tsserver.enableJavascript": true,
  "python.autoComplete.extraPaths": [
    "/home/ayman/.cache/fakegir/"
  ],
  "python.jediEnabled": false,
  "python.linting.enabled": false,
  "python.analysis.diagnosticEnabled": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "s": "openInSplit",
    "v": "openInVSplit"
  },
  "coc.source.emoji.filetypes": null,
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "args": ["--background-index"],
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "efm": {
      "command": "efm-langserver",
      "args": ["-c", "$HOME/.config/efm-langserver/config.yaml"],
      "filetypes": [
        "vim",
        "eruby",
        "markdown"
      ]
    },
    "vala" : {
      "command" : "vala-language-server",
      "filetypes" : [ "vala" ]
    },
    "swift" : {
      "command": "sourcekit-lsp",
      "rootPatterns": ["Package.swift", ".git/"],
      //"args": ["--log-level", "debug", "-Xswiftc", "-DXcode", "-Xcxx", "-I/usr/lib/swift", "-Xcxx", "-I/usr/lib/swift/Block"],
      "filetypes": ["swift"],
      //"trace.server": "verbose",
      "initializationOptions": {},
      "settings": {}
    }
  }
}
